<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>EasyTcp.Actions</PackageId>
        <Title>EasyTcp.Actions</Title>
        <Description>Support for EasyTcp to triggering specific functions with an attribute based on received data. See github for examples.</Description>
        <PackageVersion>4.0.2</PackageVersion>
        <Authors>Job79</Authors>
        <Copyright>Job79</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/Job79/EasyTcp</PackageProjectUrl>
        <RepositoryUrl>https://github.com/Job79/EasyTcp</RepositoryUrl>
        <PackageTags>Socket Networking Tcp TcpClient TcpServer Server SimpleTcp EasyTcp AsyncTcp Reflection Actions</PackageTags>
        <PackageIconUrl>https://raw.githubusercontent.com/Job79/EasyTcp/master/icon.png</PackageIconUrl>
        <TargetFrameworks>netstandard2.0;net5.0;net6.0</TargetFrameworks>

        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <NoWarn>1591</NoWarn>
        <LangVersion>8</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="EasyTcp" Version="4.0.1" />
    </ItemGroup>
</Project>
